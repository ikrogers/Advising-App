<% @currentuser = User.find_by_id(session[:user_id]) %>
<li><%= button_to 'Logout',  logout_path,:class => "btn btn-outline-inverse btn-block",  method: :delete %></li>

<%case @currentuser.classification%>
<%      when "Admin"%>
<li><%= button_to 'New Course', new_course_path,:class => "btn btn-outline-inverse btn-block",method: :get %></li>
<li><%= button_to 'Back', admin_path(),:class => "btn btn-outline-inverse btn-block" ,method: :get %></li>
<%      when "Advisor"%>
<li><%= button_to 'Back', advisor_path(),:class => "btn btn-outline-inverse btn-block",method: :get  %></li>
<%     else%>
<li><%= button_to 'Back', student_path(),:class => "btn btn-outline-inverse btn-block",method: :get  %></li>
<%     end%>
