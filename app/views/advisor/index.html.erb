<h1>Advising Center</h1>
<% @user = User.find_by_id(session[:user_id])
%>

<h2 style="color: white">Welcome <%= @user.name%>!</h2>

<p>
<%= button_to 'Manage Users', users_path(), :class => "btn btn-outline-inverse btn-block", method: :get %>
</p>

<%= button_to 'Appointments', appointments_path(), :class => "btn btn-outline-inverse btn-block", method: :get %>
</p>
<p>
<%= button_to 'View Messages', users_path("value", :value => true), :class => "btn btn-outline-inverse btn-block", method: :get %>
</p>

<p>
<%= button_to 'Logout', logout_path, :class => "btn btn-outline-inverse btn-block", method: :delete %>

</p>